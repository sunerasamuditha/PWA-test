{
  "name": "wecare-pwa",
  "version": "1.0.0",
  "description": "WeCare Progressive Web Application - Patient Portal System",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install:all": "npm install && npm install --workspace=client && npm install --workspace=server",
    "dev:client": "npm run dev --workspace=client",
    "dev:server": "npm run dev --workspace=server",
    "dev:all": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build:client": "npm run build --workspace=client",
    "build:server": "npm run build --workspace=server",
    "build:all": "npm run build:client && npm run build:server",
    "start": "npm start --workspace=server"
  },
  "keywords": [
    "pwa",
    "healthcare",
    "patient-portal",
    "react",
    "nodejs",
    "express",
    "mysql"
  ],
  "author": "WeCare Healthcare Services",
  "license": "PROPRIETARY",
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
